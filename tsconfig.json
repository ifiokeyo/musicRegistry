{
    "compilerOptions": {
        "target": "ES2019",
        "module": "commonjs",
        "allowJs": true,
        "sourceMap": true,
        "outDir": "./dist",
        "strict": true,
        "noImplicitAny": false,
        "strictNullChecks": false,
        "baseUrl": "./",
        "typeRoots": ["node_modules/@types"],
        "esModuleInterop": true
    },
    "exclude": [
        "node_modules",
        "./node_modules/*"
      ]
}
